{
    "name": "LenderHub/services",
    "description": "Service API connector between LHP services",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "firebase/php-jwt": "^5.0",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LHP\\Services\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LHP\\Services\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LHP\\Services\\ApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    }
}
